#: src/msmtp.c:2431
#, c-format
msgid ""
"Usage:\n"
"\n"
msgstr ""
"Benutzung:\n"
"\n"

#: src/msmtp.c:2432
#, c-format
msgid ""
"Sendmail mode (default):\n"
"  %s [option...] [--] recipient...\n"
"  %s [option...] -t [--] [recipient...]\n"
"  Read a mail from standard input and transmit it to an SMTP or LMTP "
"server.\n"
msgstr ""
"Sendmail Modus (Standard):\n"
"  %s [Option...] [--] Empfänger...\n"
"  %s [Option...] -t [--] [Empfänger...]\n"
"  Lese Mail von Standardeing. und übertrage sie zu einem SMTP oder LMTP "
"Server.\n"

#: src/msmtp.c:2437
#, c-format
msgid ""
"Server information mode:\n"
"  %s [option...] --serverinfo\n"
"  Print information about a server.\n"
msgstr ""
"Serverinformationsmodus:\n"
"  %s [Option...] --serverinfo\n"
"  Zeige Informationen zu einem Server.\n"

#: src/msmtp.c:2440
#, c-format
msgid ""
"Remote Message Queue Starting mode:\n"
"  %s [option...] --rmqs=host|@domain|#queue\n"
"  Send a Remote Message Queue Starting request to a server.\n"
"\n"
msgstr ""
"Remote Message Queue Starting (RMQS) Modus:\n"
"  %s [Option...] --rmqs=host|@domain|#queue\n"
"  Sende eine RMQS Anfrage an einen Server.\n"
"\n"

#: src/msmtp.c:2444
#, c-format
msgid "General options:\n"
msgstr "Allgemeine Optionen:\n"

#: src/msmtp.c:2445
#, c-format
msgid "  --version                    print version\n"
msgstr "  --version                    Zeige Version\n"

#: src/msmtp.c:2446
#, c-format
msgid "  --help                       print help\n"
msgstr "  --help                       Zeige Hilfetext\n"

#: src/msmtp.c:2447
#, c-format
msgid "  -P, --pretend                print configuration info and exit\n"
msgstr "  -P, --pretend                Zeige Konfiguration und beende\n"

#: src/msmtp.c:2448
#, c-format
msgid "  -d, --debug                  print debugging information\n"
msgstr "  -d, --debug                  Zeige Debugging Information\n"

#: src/msmtp.c:2449
#, c-format
msgid "Changing the mode of operation:\n"
msgstr "Operationsmodus wechseln:\n"

#: src/msmtp.c:2450
#, c-format
msgid "  -S, --serverinfo             print information about the server\n"
msgstr "  -S, --serverinfo             Zeige Informationen zu einem Server\n"

#: src/msmtp.c:2451
#, c-format
msgid ""
"  --rmqs=host|@domain|#queue   send a Remote Message Queue Starting request\n"
msgstr "  --rmqs=host|@domain|#queue   Sende eine RMQS Anfrage\n"

#: src/msmtp.c:2452
#, c-format
msgid "Configuration options:\n"
msgstr "Konfigurationsoptionen:\n"

#: src/msmtp.c:2453
#, c-format
msgid "  -C, --file=filename          set configuration file\n"
msgstr "  -C, --file=filename          Setze Konfigurationsdatei\n"

#: src/msmtp.c:2454
#, c-format
msgid ""
"  -a, --account=id             use the given account instead of the account\n"
"                               named \"default\"; its settings may be "
"changed\n"
"                               with command-line options\n"
msgstr ""
"  -a, --account=id             Nutze den gegebenen Account anstatt den\n"
"                               Account \"default\"; Einstellungen können "
"via\n"
"                               Kommandozeile geändert werden\n"

#: src/msmtp.c:2457
#, c-format
msgid ""
"  --host=hostname              set the server, use only command-line "
"settings;\n"
"                               do not use any configuration file data\n"
msgstr ""
"  --host=hostname              Setze den Server, nutze nur Kommandozeile;\n"
"                               nutze keine Konfigurationsdatei\n"

#: src/msmtp.c:2459
#, c-format
msgid "  --port=number                set port number\n"
msgstr "  --port=number                Setze Portnummer\n"

#: src/msmtp.c:2460
#, c-format
msgid "  --timeout=(off|seconds)      set/unset network timeout in seconds\n"
msgstr "  --timeout=(off|seconds)      Setze/lösche Timeout in Sekunden\n"

#: src/msmtp.c:2461
#, c-format
msgid "  --protocol=(smtp|lmtp)       use the given sub protocol\n"
msgstr "  --protocol=(smtp|lmtp)       Nutze das gegebene Protokoll\n"

#: src/msmtp.c:2462
#, c-format
msgid ""
"  --domain=string              set the argument of EHLO or LHLO command\n"
msgstr ""
"  --domain=string              Setze das Argument des EHLO oder LHLO "
"Kommandos\n"

#: src/msmtp.c:2463
#, c-format
msgid ""
"  --auth[=(on|off|method)]     enable/disable authentication and optionally\n"
"                               choose the method\n"
msgstr ""
"  --auth[=(on|off|method)]     (De-)aktiviere Authentifizierung und/oder\n"
"                               wähle die Methode\n"

#: src/msmtp.c:2465
#, c-format
msgid "  --user=[username]            set/unset user name for authentication\n"
msgstr ""
"  --user=[username]            Setze/lösche Nutzernamen für "
"Authentifizierung\n"

#: src/msmtp.c:2466
#, c-format
msgid "  --passwordeval=[eval]        evaluate password for authentication\n"
msgstr ""
"  --passwordeval=[eval]        Evaluaiere Passwort für Authentifizierung\n"

#: src/msmtp.c:2467
#, c-format
msgid "  --tls[=(on|off)]             enable/disable TLS encryption\n"
msgstr "  --tls[=(on|off)]             (De-)aktiviere TLS Verschlüsselung\n"

#: src/msmtp.c:2468
#, c-format
msgid "  --tls-starttls[=(on|off)]    enable/disable STARTTLS for TLS\n"
msgstr "  --tls-starttls[=(on|off)]    (De-)aktiviere STARTTLS für TLS\n"

#: src/msmtp.c:2469
#, c-format
msgid "  --tls-trust-file=[file]      set/unset trust file for TLS\n"
msgstr "  --tls-trust-file=[file]      Setze/losche Vertrauensdatei für TLS\n"

#: src/msmtp.c:2470
#, c-format
msgid "  --tls-crl-file=[file]        set/unset revocation file for TLS\n"
msgstr "  --tls-crl-file=[file]        Setze/lösche Widerrufsdatei für TLS\n"

#: src/msmtp.c:2471
#, c-format
msgid ""
"  --tls-fingerprint=[f]        set/unset trusted certificate fingerprint for "
"TLS\n"
msgstr ""
"  --tls-fingerprint=[f]        Setze/lösche Zertifikatsfingerabdruck für "
"TLS\n"

#: src/msmtp.c:2472
#, c-format
msgid "  --tls-key-file=[file]        set/unset private key file for TLS\n"
msgstr ""
"  --tls-key-file=[file]        Setze/lösche private Schlüsseldatei für TLS\n"

#: src/msmtp.c:2473
#, c-format
msgid "  --tls-cert-file=[file]       set/unset private cert file for TLS\n"
msgstr ""
"  --tls-cert-file=[file]       Setze/lösche private Zertifikatsdatei für "
"TLS\n"

#: src/msmtp.c:2474
#, c-format
msgid ""
"  --tls-certcheck[=(on|off)]   enable/disable server certificate checks for "
"TLS\n"
msgstr ""
"  --tls-certcheck[=(on|off)]   (De-)aktiviere Server-Zertifikatschecks für "
"TLS\n"

#: src/msmtp.c:2475
#, c-format
msgid "  --tls-min-dh-prime-bits=[b]  set/unset minimum bit size of DH prime\n"
msgstr ""
"  --tls-min-dh-prime-bits=[b]  Setze/lösche minimale Bitzahl für DH "
"Primzahl\n"

#: src/msmtp.c:2476
#, c-format
msgid "  --tls-priorities=[prios]     set/unset TLS priorities.\n"
msgstr "  --tls-priorities=[prios]     Setze/lösche TLS Prioritäten.\n"

#: src/msmtp.c:2477
#, c-format
msgid "Options specific to sendmail mode:\n"
msgstr "Optionen für den Sendmail Modus:\n"

#: src/msmtp.c:2478
#, c-format
msgid ""
"  --auto-from[=(on|off)]       enable/disable automatic envelope-from "
"addresses\n"
msgstr ""
"  --auto-from[=(on|off)]       (De-)aktiviere autom. Umschlags-"
"Absenderadresse\n"

#: src/msmtp.c:2479
#, c-format
msgid "  -f, --from=address           set envelope from address\n"
msgstr "  -f, --from=address           Setze Umschlags-Absenderadresse\n"

#: src/msmtp.c:2480
#, c-format
msgid ""
"  --maildomain=[domain]        set the domain for automatic envelope from\n"
"                               addresses\n"
msgstr ""
"  --maildomain=[domain]        Setze die Domain für autom. Umschlags-\n"
"                               Absenderadresse\n"

#: src/msmtp.c:2482
#, c-format
msgid "  -N, --dsn-notify=(off|cond)  set/unset DSN conditions\n"
msgstr "  -N, --dsn-notify=(off|cond)  Setze/lösche DSN Bedingungen\n"

#: src/msmtp.c:2483
#, c-format
msgid "  -R, --dsn-return=(off|ret)   set/unset DSN amount\n"
msgstr "  -R, --dsn-return=(off|ret)   Setze/lösche DSN Anteil\n"

#: src/msmtp.c:2484
#, c-format
msgid "  -X, --logfile=[file]         set/unset log file\n"
msgstr "  -X, --logfile=[file]         Setze/lösche Logdatei\n"

#: src/msmtp.c:2485
#, c-format
msgid ""
"  --syslog[=(on|off|facility)] enable/disable/configure syslog logging\n"
msgstr ""
"  --syslog[=(on|off|facility)] (De-)aktiviere/Konfiguriere Systemlogging\n"

#: src/msmtp.c:2486
#, c-format
msgid ""
"  -t, --read-recipients        read additional recipients from the mail\n"
msgstr "  -t, --read-recipients        Lese weitere Empfänger aus der Mail\n"

#: src/msmtp.c:2487
#, c-format
msgid ""
"  --read-envelope-from         read envelope from address from the mail\n"
msgstr "  --read-envelope-from         Lese Umschlags-Absender aus der Mail\n"

#: src/msmtp.c:2488
